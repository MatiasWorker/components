{
    "name": "@bxreact/form",
    "version": "1.7.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "build": "exports \"src/**/*.{tsx,ts}\" --exports --css-import-source @bxreact/theme/css --types --main index",
        "exports": "exports \"src/**/*.{tsx,ts}\" --exports --css-import-source @bxreact/theme/css --types --publish --main index"
    },
    "license": "ISC",
    "dependencies": {
        "@bxreact/theme": "*",
        "@bxreact/grid": "*",
        "@bxreact/label": "*",
        "@bxreact/input": "*"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2",
        "@types/react": ">=17.0.38"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "module": "./dist/index.js",
    "exports": {
        "./index": "./dist/index.js",
        ".": "./dist/index.js",
        "./types": "./dist/types.js",
        "./utils": "./dist/utils.js"
    },
    "types": "types/index.d.ts",
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ],
            "types": [
                "./types/types.d.ts"
            ],
            "utils": [
                "./types/utils.d.ts"
            ]
        }
    }
}

