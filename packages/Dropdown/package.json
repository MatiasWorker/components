{
    "name": "@bxreact/dropdown",
    "version": "1.4.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "build": "exports src/**/*.tsx --exports --css-import-source @bxreact/theme/css --types --main index",
        "exports": "exports src/**/*.tsx --exports --css-import-source @bxreact/theme/css --types --publish --main index"
    },
    "license": "ISC",
    "dependencies": {
        "@bxreact/theme": "*",
        "@floating-ui/react-dom": "1.0.0"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2",
        "@types/react": ">=17.0.38"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "module": "./dist/index.js",
    "exports": {
        "./index": "./dist/index.js",
        ".": "./dist/index.js"
    },
    "types": "types/index.d.ts",
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ]
        }
    }
}
