{
    "name": "@bxreact/input",
    "version": "1.3.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "build": "exports src/**/*.tsx --exports --jsx react --css-inline --types --main index",
        "exports": "exports src/**/*.tsx --exports --jsx react --css-inline --types --main index --publish"
    },
    "license": "ISC",
    "dependencies": {
        "@bxreact/icon": "*",
        "@bxreact/theme": "*",
        "ustyler": "*",
        "classnames": "*",
        "@types/classnames": "*",
        "@types/react-date-range": "^1.4.4",
        "react-date-range": "^1.4.0"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2",
        "@types/react": ">=17.0.38"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "module": "./dist/index.js",
    "types": "types/index.d.ts",
    "exports": {
        "./index": "./dist/index.js",
        ".": "./dist/index.js",
        "./Textarea": "./dist/Textarea.js"
    },
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ],
            "Textarea": [
                "./types/Textarea.d.ts"
            ]
        }
    }
}
