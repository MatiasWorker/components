{
    "name": "@bxreact/date-picker",
    "version": "2.1.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "build": "exports src/**/*.tsx --exports --css-import-source @bxreact/theme/css --types --main index",
        "exports": "exports src/**/*.tsx --exports --css-import-source @bxreact/theme/css --types --publish --main index"
    },
    "license": "ISC",
    "dependencies": {
        "@bxreact/theme": "*",
        "@bxreact/input": "*",
        "@bxreact/dropdown": "*",
        "@bxreact/icon": "*"
    },
    "peerDependencies": {
        "react": ">=17.0.2",
        "react-dom": ">=17.0.2",
        "@types/react": ">=17.0.38",
        "react-date-range": "^1.4.0",
        "@types/react-date-range": "^1.4.4"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "module": "./dist/index.js",
    "exports": {
        "./index": "./dist/index.js",
        ".": "./dist/index.js"
    },
    "types": "types/index.d.ts",
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ]
        }
    }
}
