{
    "name": "@bxreact/use-pages",
    "version": "1.1.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "exports": "exports src/index.tsx --exports --jsx react --types",
        "prepublishOnly": "npm run exports"
    },
    "license": "ISC",
    "dependencies": {
        "react": "latest",
        "react-dom": "latest",
        "@types/react": "latest"
    },
    "peerDependencies": {
        "react": "latest",
        "react-dom": "latest",
        "@types/react": "latest"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "types": "types/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./index": "./dist/index.js"
    },
    "module": "./dist/index.js",
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ]
        }
    }
}
