{
    "name": "@bxreact/theme",
    "version": "2.2.0",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "build": "exports src/**/*.ts --exports --css-import-source @bxreact/css --types --main index",
        "exports": "exports src/**/*.ts --exports --css-import-source @bxreact/css --types --publish --main index"
    },
    "license": "ISC",
    "dependencies": {
        "@bxreact/css": "*"
    },
    "devDependencies": {
        "@bxreact/tsconfig": "*"
    },
    "module": "./dist/index.js",
    "exports": {
        "./index": "./dist/index.js",
        ".": "./dist/index.js",
        "./css": "./dist/css.js"
    },
    "types": "types/index.d.ts",
    "typesVersions": {
        "*": {
            "index": [
                "./types/index.d.ts"
            ],
            "css": [
                "./types/css.d.ts"
            ]
        }
    }
}